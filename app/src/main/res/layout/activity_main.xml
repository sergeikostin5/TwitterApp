<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sergeikostin.demoapp.ui.home_screen.HomeActivity">

    <fragment
        android:id="@+id/home_timeline_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.sergeikostin.demoapp.ui.home_screen.timeline.TimeLineFragment"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/new_tweet_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="16dp"
        android:src="@drawable/ic_new_tweet"/>

</FrameLayout>
